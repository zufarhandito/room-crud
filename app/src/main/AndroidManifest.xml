<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.shop.bestshop">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.shop.bestshop.ShopApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_cat"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_cat_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.shop">
        <activity android:name="com.shop.bestshop.MenuActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.shop.bestshop.EditActivity"
            android:parentActivityName="com.shop.bestshop.MainActivity"
            ></activity>
        <activity
            android:name="com.shop.bestshop.MainActivity"
            android:parentActivityName="com.shop.bestshop.MenuActivity">
        </activity>
        <activity
            android:name="com.shop.bestshop.aboutActivity"
            android:parentActivityName="com.shop.bestshop.MenuActivity"
            ></activity>
    </application>


</manifest>